<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/drawer_layout"
    android:fitsSystemWindows="true"
    tools:openDrawer="start"
    tools:context=".MainActivity">

    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:headerLayout="@layout/header"
        app:menu="@menu/menu"
        android:id="@+id/nav_view"
        android:layout_gravity="start"
        app:itemTextColor="#664772"
        app:drawerLayoutCornerSize="80dp"
        app:itemRippleColor="#362F39"
        android:background="#D5CDD8"
        android:theme="@style/Theme.Material3.Light.NoActionBar"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="717dp"
        android:layout_height="382dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/pp3" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="311dp"
        android:layout_height="229dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pp4" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="178dp"
        android:layout_height="127dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.816"
        app:srcCompat="@drawable/girl3" />

    <ImageView
        android:id="@+id/form"
        android:layout_width="170dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:layout_constraintVertical_bias="0.157"
        app:srcCompat="@drawable/form" />

    <ImageView
        android:id="@+id/doctors"
        android:layout_width="170dp"
        android:layout_height="200dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.116"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/form"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/doctors" />

    <ImageView
        android:id="@+id/results"
        android:layout_width="184dp"
        android:layout_height="472dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/doctors"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/result2" />

    <TextView
        android:id="@+id/results_and_chart"
        android:layout_width="119dp"
        android:layout_height="144dp"
        android:text="@string/results"
        android:textAlignment="center"
        android:textColor="#63507B"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/results"
        app:layout_constraintEnd_toEndOf="@id/results"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="@id/results"
        app:layout_constraintTop_toTopOf="@id/results"
        app:layout_constraintVertical_bias="0.773" />


    <TextView
        android:id="@+id/doctorss"
        android:layout_width="127dp"
        android:layout_height="46dp"
        android:text="@string/doctors"
        android:textAlignment="center"
        android:textColor="#63507B"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/doctors"
        app:layout_constraintEnd_toEndOf="@id/doctors"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="@id/doctors"
        app:layout_constraintTop_toTopOf="@id/doctors"
        app:layout_constraintVertical_bias="0.837" />

    <TextView
        android:id="@+id/formm"
        android:layout_width="131dp"
        android:layout_height="45dp"
        android:text="@string/form"
        android:textAlignment="center"
        android:textColor="#63507B"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/form"
        app:layout_constraintEnd_toEndOf="@id/form"
        app:layout_constraintHorizontal_bias="0.54"
        app:layout_constraintStart_toStartOf="@id/form"
        app:layout_constraintTop_toTopOf="@id/form"
        app:layout_constraintVertical_bias="0.773" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="113dp"
        android:layout_height="61dp"
        android:text="Welcome"
        android:textColor="#63507B"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/form"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView11"
        app:layout_constraintVertical_bias="0.0" />

    <ProgressBar
        android:visibility="gone"
        android:id="@+id/progressBar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.drawerlayout.widget.DrawerLayout>